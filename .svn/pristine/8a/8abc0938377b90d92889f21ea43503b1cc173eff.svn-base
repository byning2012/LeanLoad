<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/dim600"
    android:layout_height="@dimen/dim440"
    android:background="@drawable/dialog_bg_corner"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim90">

        <TextView
            android:id="@+id/tv_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:textColor="@color/tc_black"
            android:textSize="@dimen/txt36"
            android:textStyle="bold" />
    <RelativeLayout
        android:id="@+id/rl_close"
        android:layout_width="@dimen/dim90"
        android:layout_height="match_parent"
        android:layout_alignParentRight="true">
        <ImageView
            android:id="@+id/iv_delete"
            android:layout_width="@dimen/dim28"
            android:layout_height="@dimen/dim28"
            android:layout_centerInParent="true"
            android:layout_marginRight="@dimen/dim20"
            android:src="@drawable/perturbation" />
    </RelativeLayout>
    </RelativeLayout>


    <include layout="@layout/divide_line" />

    <TextView
        android:id="@+id/tv_bind_time"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dim40"
        android:layout_marginLeft="@dimen/dim44"
        android:textColor="@color/delete_txt"
        android:textSize="@dimen/txt28" />

    <Button
        android:id="@+id/btn_delete"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim90"
        android:layout_marginBottom="@dimen/dim60"
        android:layout_marginLeft="@dimen/dim20"
        android:layout_marginRight="@dimen/dim20"
        android:layout_marginTop="@dimen/dim100"
        android:background="@drawable/select_sure_register_btn"
        android:text="@string/delete"
        android:textColor="@color/select_white_orange"
        android:textSize="@dimen/txt30" />
</LinearLayout>