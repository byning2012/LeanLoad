<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="13dp" />
            <gradient
                android:centerColor="@color/app_bg"
                android:endColor="@color/app_bg"
                android:startColor="@color/app_bg" />
            <stroke
                android:width="1dp"
                android:color="@color/seek_bar_strock" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="13dp" />
                <gradient
                    android:centerColor="@color/home_blue"
                    android:endColor="@color/home_blue"
                    android:startColor="@color/home_blue" />
                <stroke
                    android:width="1dp"
                    android:color="@color/seek_bar_strock" />
            </shape>
        </clip>
    </item>
</layer-list>