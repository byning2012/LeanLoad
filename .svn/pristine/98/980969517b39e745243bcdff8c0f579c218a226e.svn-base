<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitleLayout">
        <attr name="titlelayout_title" format="reference|string" />
        <attr name="titlelayout_showback" format="boolean" />
        <attr name="titlelayout_style" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TabStripView">
        <attr name="navigateTabTextColor" format="color|reference" />
        <attr name="navigateTabSelectedTextColor" format="color|reference" />
        <attr name="containerId" format="integer|reference" />
        <attr name="navigateTabTextSize" format="dimension|reference" />
    </declare-styleable>

    <!--自定义seekbar-->
    <declare-styleable name="NumTipSeekBar">
        <attr name="tickBarHeight" format="dimension" />
        <attr name="tickBarColor" format="color" />
        <attr name="circleButtonColor" format="color" />
        <attr name="circleButtonTextColor" format="color" />
        <attr name="circleButtonTextSize" format="dimension" />
        <attr name="circleButtonRadius" format="dimension" />
        <attr name="circleApertureWidth" format="dimension" />
        <attr name="circleApertureColor" format="color" />
        <attr name="progressHeight" format="dimension" />
        <attr name="progressColor" format="color" />
        <attr name="selectProgress" format="integer" />
        <attr name="startProgress" format="integer" />
        <attr name="maxProgress" format="integer" />
        <attr name="isShowButtonText" format="boolean" />
        <attr name="isShowButton" format="boolean" />
        <attr name="isRound" format="boolean" />
    </declare-styleable>
</resources>