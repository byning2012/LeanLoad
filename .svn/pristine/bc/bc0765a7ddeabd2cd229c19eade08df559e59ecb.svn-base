<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_audit_results"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.hzxm.easyloan.ui.activity.AuditResultsActivity">

    <com.lmz.baselibrary.widget.TitleLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:titlelayout_showback="true"
        app:titlelayout_style="true"
        app:titlelayout_title="@string/audit_result" />

    <include layout="@layout/divide_line" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/dim160"
        android:src="@drawable/auditing" />

    <TextView
        android:id="@+id/tv_audit_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/dim40"
        android:text="@string/submit_success"
        android:textColor="@color/tc_black"
        android:textSize="@dimen/txt30" />

    <TextView
        android:id="@+id/tv_audit_result_msg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/dim40"
        android:text="@string/wait_audit_result"
        android:textColor="@color/deep_text_white"
        android:textSize="@dimen/txt30" />

    <Button
        android:id="@+id/btn_complete"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim80"
        android:layout_marginLeft="@dimen/dim30"
        android:layout_marginRight="@dimen/dim30"
        android:layout_marginTop="@dimen/dim100"
        android:background="@drawable/select_sure_register_btn"
        android:text="@string/complete"
        android:textColor="@color/select_white_orange"
        android:textSize="@dimen/txt34" />
</LinearLayout>
